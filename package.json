{
  "name": "drawchange",
  "description": "a really cool app",
  "version": "1.0.0",
  "cacheDirectories": [
    "frontend/node_modules",
    "backend/node_modules"
  ],
  "scripts": {
    "start": "concurrently \"cd backend/ && npm start\" \"cd frontend/ && npm start\"",
    "install-frontend": "yarn install --cwd frontend",
    "install-backend": "yarn install --cwd backend",
    "build-frontend": "cd frontend/ && yarn build",
    "postinstall": "yarn install-frontend && yarn install-backend && yarn build-frontend"
  },
  "engines": {
    "yarn": "1.12.3",
    "node": "10.14.2"
  },
  "dependencies": {
    "react": "^16.7.0",
    "react-plaid-link": "^1.0.3",
    "react-pure-modal": "^1.5.1",
    "reactjs-popup": "^1.3.2"
  },
  "devDependencies": {
    "concurrently": "^4.1.0"
  }
}
